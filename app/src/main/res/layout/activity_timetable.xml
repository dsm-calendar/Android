    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view_timetable_background"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toTopOf="parent"
        android:background="#6cccde"/>

    <ImageButton
        android:id="@+id/button_timetable_off"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/view_timetable_background"
        app:layout_constraintStart_toStartOf="@id/view_timetable_background"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:src="@drawable/ic_cross_out"
        android:background="@null"/>

    <TextView
        android:id="@+id/tv_timetable_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/view_timetable_background"
        app:layout_constraintBottom_toBottomOf="@id/view_timetable_background"
        app:layout_constraintStart_toStartOf="@id/view_timetable_background"
        app:layout_constraintEnd_toEndOf="@id/view_timetable_background"
        android:text="전체 시간표"
        android:textSize="20dp"
        android:textColor="#FFFFFF"
        android:fontFamily="@font/bm_dohyun"/>

    <ImageButton
        android:id="@+id/button_timetable_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/view_timetable_background"
        app:layout_constraintBottom_toBottomOf="@id/view_timetable_background"
        app:layout_constraintEnd_toEndOf="@id/view_timetable_background"
        android:layout_marginEnd="20dp"
        android:src="@drawable/ic_pencil_white"
        android:background="@null"/>

    <RadioGroup
        android:id="@+id/rg_timetable_grade"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        app:layout_constraintTop_toBottomOf="@id/view_timetable_background"
        android:orientation="horizontal">

        <RadioButton
            android:id="@+id/button_timetable_grade_1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/radio_states_time_table"
            android:checked="true"
            android:gravity="center"
            android:textColor="#2898ad"
            android:fontFamily="@font/bm_dohyun"
            android:button="@null"
            android:onClick="setGrade"
            android:text="1학년"/>

        <RadioButton
            android:id="@+id/button_timetable_grade_2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/radio_states_time_table"
