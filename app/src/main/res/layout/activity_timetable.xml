        android:id="@+id/rg_timetable_class"
        android:layout_width="360dp"
        android:layout_height="20dp"
        app:layout_constraintTop_toBottomOf="@id/rg_timetable_grade"
        app:layout_constraintStart_toStartOf="parent"
