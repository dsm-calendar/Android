    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view_time_table_background"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintTop_toTopOf="parent"
        android:background="#6cccde"/>

    <ImageButton
        android:id="@+id/button_time_table_off"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/view_time_table_background"
        app:layout_constraintStart_toStartOf="@id/view_time_table_background"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:src="@drawable/ic_cross_out"
        android:background="@null"
        android:contentDescription="@string/back" />

    <TextView
        android:id="@+id/tv_time_table_whole_time_table"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/view_time_table_background"
        app:layout_constraintBottom_toBottomOf="@id/view_time_table_background"
        app:layout_constraintStart_toStartOf="@id/view_time_table_background"
        app:layout_constraintEnd_toEndOf="@id/view_time_table_background"
        android:text="@string/whole_timeTable"
        android:textSize="20dp"
        android:textColor="#FFFFFF"
        android:fontFamily="@font/bm_dohyun"/>

    <RadioGroup
        android:id="@+id/radio_group_grade"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        app:layout_constraintTop_toBottomOf="@id/view_time_table_background"
        android:orientation="horizontal">

        <RadioButton
            android:id="@+id/radioButton_grade_1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/radio_states_time_table"
            android:checked="true"
            android:gravity="center"
            android:textColor="#2898ad"
            android:fontFamily="@font/bm_dohyun"
            android:button="@null"
            android:text="@string/first_grade"/>

        <RadioButton
            android:id="@+id/radioButton_grade_2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/radio_states_time_table"
            android:gravity="center"
            android:textColor="#2898ad"
            android:fontFamily="@font/bm_dohyun"
            android:button="@null"
            android:text="@string/second_grade"/>

